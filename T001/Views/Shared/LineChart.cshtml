@using Chart.Mvc.Extensions
@using Chart.Mvc.ComplexChart

@model T001.Models.GraphData

@{
    const string Canvas = "LineChart";
    var lineChart = new LineChart();
    string id = Model.LabelText.Replace(" ", ""); 

    lineChart.ComplexData.Labels.AddRange(Model.Label);
    lineChart.ComplexData.Datasets.AddRange(Model.Datasets);
}
<canvas id="@id" width="850" height="400"></canvas>

@Html.CreateChart(id, lineChart)
